Attribute VB_Name = "OptionsStorage"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim PLCStn As Integer
Dim EditEnabled As Boolean
Dim MaxAreas As Integer

Private Sub cmbPLC_Click()
    Call Config(cmbPLC.ItemData(cmbPLC.ListIndex))
End Sub

Private Sub Cmd_exit_Click()
    Unload Me
End Sub

Public Sub Config(PLC As Integer)
    
    
    
    PLCStn = PLC
    
    EditEnabled = (AccessLevel >= accEngineer)
    
    
    
    'Set Max Areas to 4 if it has not been set
    MaxAreas = StorageAreaMax(PLCStn)
    If (MaxAreas = 0) Or (MaxAreas > 20) Then MaxAreas = 8
    
    
    Dim lbl As Label
    For Each lbl In lblNum()
        
        txtAreaName(lbl.Index).Text = db_uspSettingGet("SettingsNetwork", "AreaName PLC" & Str(PLCStn), Format$(lbl.Index), "Area " & Format$(lbl.Index), PCNum)
        
        txtLineFrom(lbl.Index).Text = Format$(StorageAreaFrom(lbl.Index, PLCStn))
        txtLineTo(lbl.Index).Text = Format$(StorageAreaTo(lbl.Index, PLCStn))
                
        lblNum(lbl.Index).Visible = (lbl.Index <= MaxAreas)
        txtAreaName(lbl.Index).Visible = (lbl.Index <= MaxAreas)
        txtLineFrom(lbl.Index).Visible = (lbl.Index <= MaxAreas)
        txtLineTo(lbl.Index).Visible = (lbl.Index <= MaxAreas)
    Next lbl
    
    Me.cmd_Send.Top = lblNum(MaxAreas).Top + 540
    Me.Cmd_exit.Top = Me.cmd_Send.Top + 450
    Me.Height = Me.Cmd_exit.Top + Me.Cmd_exit.Height + 540
    
    cmbPLC.Clear
    LoadPLCs
    lblPLC.Caption = " " & Format$(PLCStn, "#0") & " -  " & PLCName(PLCStn)
    cmbPLC.Visible = False
    
    EnableBoxes
    
End Sub
Private Sub LoadPLCs()
      
    Dim PLC As Integer
        
    For PLC = 0 To UsedPLC
        cmbPLC.AddItem " " & Format$(PLC, "#0") & " -  " & PLCName(PLC)
        cmbPLC.ItemData(cmbPLC.NewIndex) = PLC
    Next PLC
   
End Sub

Private Sub Cmd_send_Click()
    
    Dim lbl As Label
    For Each lbl In lblNum()
        db_uspSettingSave "SettingsNetwork", "AreaName PLC" & Str(PLCStn), Format$(lbl.Index), txtAreaName(lbl.Index).Text, PCNum
    Next lbl
    
    Dim n           As Integer
    Dim myFromLine  As Integer
    
    For n = 1 To MaxAreas
        Data(n - 1) = Val(txtLineFrom(n))
    Next n
    
    Comms_frm.WritePLC PLCStn, "D", 251, MaxAreas, Data(), False, "OptionsStorage"
    
    For n = 1 To MaxAreas
        'If the From line is greater then the To line then set the same
        If (Val(txtLineTo(n)) < Val(txtLineFrom(n))) Then
            Data(n - 1) = Val(txtLineFrom(n))
        Else
            Data(n - 1) = Val(txtLineTo(n))
        End If
        
    Next n
    
    Comms_frm.WritePLC PLCStn, "D", 271, MaxAreas, Data(), False, "OptionsStorage"
    
End Sub

Private Sub Form_Load()
    
    CenterForm Me
    Me.Top = 1000
    
    SetLanguage

End Sub

Private Sub lblPLC_Click()
    cmbPLC.Text = lblPLC.Caption
    cmbPLC.Visible = True
    
End Sub
Private Sub EnableBoxes()
       
    Dim lbl As Label
    
    For Each lbl In lblNum()
        txtAreaName(lbl.Index).Enabled = EditEnabled
        txtLineFrom(lbl.Index).Enabled = EditEnabled
        txtLineTo(lbl.Index).Enabled = EditEnabled
    Next lbl
        
End Sub
Private Sub SetLanguage()
        
    Me.Cmd_exit.Caption = TextWord(309)
    Me.cmd_Send.Caption = TextWord(287)
    
    Me.Caption = TextWord(820)
    
    Me.lblArea.Caption = TextWord(821)
    Me.lblFrom.Caption = TextWord(822)
    Me.lblTo = TextWord(823)
    
End Sub

Private Sub txtLineFrom_KeyPress(Index As Integer, KeyAscii As Integer)
    BoxKeyPress txtLineFrom(Index), KeyAscii, EditLine
End Sub

Private Sub txtLineTo_KeyPress(Index As Integer, KeyAscii As Integer)
    BoxKeyPress txtLineTo(Index), KeyAscii, EditLine
End Sub
