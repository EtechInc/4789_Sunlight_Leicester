Private Sub LoadPLCs()
      
    Dim PLC As Integer
        
    For PLC = 0 To UsedPLC
        cmbPLC.AddItem " " & Format$(PLC, "#0") & " -  " & PLCName(PLC)
        cmbPLC.ItemData(cmbPLC.NewIndex) = PLC
    Next PLC
   
End Sub


Private Sub Form_Load()
    
    CenterForm Me
    Me.Top = 1000
    
    SetLanguage

End Sub


Private Sub lblPLC_Click()
    cmbPLC.Text = lblPLC.Caption
    cmbPLC.Visible = True
    
End Sub
Private Sub EnableBoxes()
       
    Dim lbl As Label
    Dim myEnable As Boolean
    
    
    
    For Each lbl In lblNum()
        
        myEnable = (EditEnabled And (lbl.Index <= PLCMaxUnloading(PLCStn)))
        
        txtName(lbl.Index).Enabled = myEnable
        
        picShowLines(lbl.Index).Enabled = myEnable
        picShowBatch(lbl.Index).Enabled = myEnable
        picShowGrp1(lbl.Index).Enabled = myEnable
        picShowGrp2(lbl.Index).Enabled = myEnable
        picShowGrp3(lbl.Index).Enabled = myEnable
        picShowCus(lbl.Index).Enabled = myEnable
        
        picMixGrp(lbl.Index).Enabled = myEnable
        picMixCus(lbl.Index).Enabled = myEnable
        picAllowBatchChange(lbl.Index).Enabled = myEnable
        
    Next lbl
        
End Sub
Private Sub SetLanguage()
        
    Me.Cmd_exit.Caption = TextWord(309)
        
    Me.Caption = TextWord(820)
    
    
    
End Sub


