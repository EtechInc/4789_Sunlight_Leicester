Attribute VB_Name = "OptionsMachines"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim PLCStn As Integer
Dim EditEnabled As Boolean

Private Sub cmbPLC_Click()
    Call Config(cmbPLC.ItemData(cmbPLC.ListIndex))
End Sub

Private Sub Cmd_exit_Click()
    Unload Me
End Sub

Public Sub Config(PLC As Integer)
        
    PLCStn = PLC
    
    EditEnabled = (AccessLevel >= accEngineer)
        
    cmbPLC.Clear
    LoadPLCs
    lblPLC.Caption = " " & Format$(PLCStn, "#0") & " -  " & PLCName(PLCStn)
    cmbPLC.Visible = False
    
    EnableBoxes
        
    Dim lbl As Label
    For Each lbl In lblNum()
        
        lblNum(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        txtName(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        
        picShowLines(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picShowBatch(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picShowGrp1(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picShowGrp2(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picShowGrp3(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picShowCus(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        
        picMixGrp(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picMixCus(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        picAllowBatchChange(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        
        picMachine(lbl.Index).Visible = (lbl.Index <= MaxUnloading)
        
        If txtName(lbl.Index).Visible And txtName(lbl.Index).Enabled Then
            txtName(lbl.Index).Text = db_uspSettingGet("SettingsNetwork", "DischargerName PLC" & Str(PLCStn), Format$(lbl.Index), "Discharger" & Str(lbl.Index), PCNum)
        Else
            txtName(lbl.Index).Text = ""
        End If
                        
        If picShowLines(lbl.Index).Visible And picShowLines(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowLine" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowLines(lbl.Index).Picture = picLampGreen.Picture Else picShowLines(lbl.Index).Picture = picLampGrey.Picture
        
        If picShowBatch(lbl.Index).Visible And picShowBatch(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowBatch" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowBatch(lbl.Index).Picture = picLampGreen.Picture Else picShowBatch(lbl.Index).Picture = picLampGrey.Picture
         
        If picShowGrp1(lbl.Index).Visible And picShowGrp1(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowProgram1" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowGrp1(lbl.Index).Picture = picLampGreen.Picture Else picShowGrp1(lbl.Index).Picture = picLampGrey.Picture
        
        If picShowGrp2(lbl.Index).Visible And picShowGrp2(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowProgram2" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowGrp2(lbl.Index).Picture = picLampGreen.Picture Else picShowGrp2(lbl.Index).Picture = picLampGrey.Picture
        
        If picShowGrp3(lbl.Index).Visible And picShowGrp3(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowProgram3" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowGrp3(lbl.Index).Picture = picLampGreen.Picture Else picShowGrp3(lbl.Index).Picture = picLampGrey.Picture
        
        If picShowCus(lbl.Index).Visible And picShowCus(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "ShowCustomers" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picShowCus(lbl.Index).Picture = picLampGreen.Picture Else picShowCus(lbl.Index).Picture = picLampGrey.Picture
        
        If picMixGrp(lbl.Index).Visible And picMixGrp(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "BatchMixProgram" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picMixGrp(lbl.Index).Picture = picLampGreen.Picture Else picMixGrp(lbl.Index).Picture = picLampGrey.Picture
        
        If picMixCus(lbl.Index).Visible And picMixCus(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "BatchMixCustomers" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picMixCus(lbl.Index).Picture = picLampGreen.Picture Else picMixCus(lbl.Index).Picture = picLampGrey.Picture
        
        If picAllowBatchChange(lbl.Index).Visible And picAllowBatchChange(lbl.Index).Enabled Then
            OptionSelected = db_uspSettingGet("SettingsNetwork", "Unloading PLC" & Str(PLCStn), "AllowBatchChange" & Str(lbl.Index), 0, PCNum)
        Else
            OptionSelected = False
        End If
        If OptionSelected Then picAllowBatchChange(lbl.Index).Picture = picLampGreen.Picture Else picAllowBatchChange(lbl.Index).Picture = picLampGrey.Picture
              
    Next lbl
       
    Me.Cmd_exit.Top = lblNum(MaxUnloading).Top + 540
    Me.Height = Me.Cmd_exit.Top + Me.Cmd_exit.Height + 540
    
    
    
